#include <stdio.h>
int main()
{
    int a[20];
    int n,max=-2,min=99999;
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        scanf("%d",a[i]);
    }
    for(int i=0;i<n;i++)
    {
        if(a[i]>max)
        {
            max=i;
        }
        if(a[i]<min)
        {
            min=i;
        }
    }
    return 0;
}