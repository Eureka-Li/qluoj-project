#include <stdio.h>

int main()
{
    int N;
    int a[25];
    scanf("%d", &N);
    for (int i = 0; i < N; i++)
    {
        scanf("%d", &a[i]);
    }

    // 循
    for (int row = 0; row < N; row++)
    {
        // 输出当前数组
        for (int i = 0; i < N; i++)
        {
            printf("%d", a[i]);
            if (i < N - 1)
                printf(" ");
        }
        printf("\n");

        // 暴力右移 1 位
        int last = a[N - 1];
        for (int i = N - 1; i > 0; i--)
        {
            a[i] = a[i - 1];
        }
        a[0] = last;
    }

    return 0;
}