#include <stdio.h>
int main()
{
    int T, n;
    long long fib[41] = {0}, sum[41] = {0};

    fib[1] = 1;
    fib[2] = 2;
    for (int i = 3; i <= 40; i++)
    {
        fib[i] = fib[i - 1] + fib[i - 2];
    }
    for (int i = 1; i <= 40; i++)
    {
        sum[i] = sum[i - 1] + fib[i];
    }
    scanf("%d", &T);
    while (T--)
    {
        scanf("%d", &n);
        if (n == 0)
        {
            printf("0\n");
        }
        else
        {
            printf("%lld\n", sum[n] + 1);
        }
    }
    return 0;
}