#include <stdio.h>
int main()
{
    long long sum = 0;
    int a[10010];
    int L, R;
    int n;
    scanf("%d", &n);
    for (int i = 0; i < n; i++)
    {
        scanf("%d", &a[i]);
    }
    scanf("%d %d", &L, &R);
    for (int j = L - 1; j <= R - 1; j++)
    {
        sum += a[j];
    }
    printf("%lld", sum);
    return 0;
}