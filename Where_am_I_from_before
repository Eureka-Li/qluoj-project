//https://oj.qlu.edu.cn/p/P1396
#include <bits/stdc++.h>
using namespace std;

int main() {
    int n;
    cin >> n;
    vector<pair<int,int>> arr(n); // {value, original_index}
    for (int i = 0; i < n; i++) {
        cin >> arr[i].first;
        arr[i].second = i + 1; // 原位置从1开始
    }

    // 按值升序排序
    sort(arr.begin(), arr.end(), [](const pair<int,int> &a, const pair<int,int> &b){
        return a.first < b.first;
    });

    // 输出排序后的值
    for (int i = 0; i < n; i++) {
        if (i) cout << " ";
        cout << arr[i].first;
    }
    cout << endl;

    // 输出排序后对应的原位置
    for (int i = 0; i < n; i++) {
        if (i) cout << " ";
        cout << arr[i].second;
    }
    cout << endl;

    return 0;
}